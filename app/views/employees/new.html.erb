<h1 class="center">Create Employee</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@employee, url: employees_path) do |f| %>
      <%= render 'form', f: f %>

      <div class="center">
        <%= f.submit "Create", class: "btn btn-primary" %>
      </div>
    <% end %>
  </div>
</div>
