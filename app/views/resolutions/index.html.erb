<div class="page-header">
  <%= link_to 'Add', new_resolution_path, data: { remote_popup: true, title: "Create Resolution" }, class: "btn btn-primary pull-right" %>
  <h3>Resolutions</h3>
</div>
<div class="table-responsive">
  <table class="table table-striped table-last">
    <tr>
      <th>Id</th>
      <th>Name</th>
      <th>&nbsp;</th>
    </tr>
    <% @resolutions.each do |resolution| %>
      <tr>
        <td><%= resolution.id %></td>
        <td><%= resolution.name %></td>
        <td>
          <%= edit_link(edit_resolution_path(resolution), "Resolution") %>
        </td>
      </tr>
    <% end %>
  </table>
</div>
<%= will_paginate %>
